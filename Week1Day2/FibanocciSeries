package week1.day2;

import java.util.Scanner;

public class FibanoacciSeries {

	    // Function to print N Fibonacci Number
	    static void Fibonacci(int N)
	    {
	        int n1 = 0, n2 = 1;

	        for (int i = 0; i < N; i++) {
	            // Print the number
	            System.out.print(n1 + " ");

	            // Swap
	            int n3 = n2 + n1;
	            n1 = n2;
	            n2 = n3;
	        }
	    }

	    // Driver Code
	    public static void main(String args[])
	    {
	        // Given Number N
	    	Scanner N1=new Scanner(System.in);
	        int N=N1.nextInt();

	        // Function Call
	        Fibonacci(N);
	    }
	}
